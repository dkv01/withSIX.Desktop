<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog" />
  </configSections>
  <startup useLegacyV2RuntimeActivationPolicy="true">
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6" />
  </startup>
  <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <extensions>
      <add assembly="LogentriesNLog" />
    </extensions>
    <targets>
      <target name="logfile" xsi:type="File" fileName="${specialfolder:folder=LocalApplicationData}/SIX Networks/Synq/logs/Synq.log" archiveFileName="${specialfolder:folder=LocalApplicationData}/SIX Networks/Synq/logs/Synq_{#####}.log" archiveAboveSize="1048576" archiveNumbering="Sequence" maxArchiveFiles="5" concurrentWrites="true" />
      <target name="logentries" type="Logentries" debug="true" httpPut="false" ssl="false" layout="${date:format=ddd MMM dd} ${time:format=HH:mm:ss} ${date:format=zzz yyyy} ${logger} : ${LEVEL}, ${message}" />
    </targets>
    <rules>
      <logger name="SN.*" minlevel="Trace" writeTo="logfile" />
      <logger name="SN.*" minlevel="Error" writeTo="logentries" final="true" />
    </rules>
  </nlog>
  <appSettings>
    <add key="Logentries.Token" value="cae22f74-4cd1-41c0-88c1-d0c836b079bb" />
  </appSettings>

