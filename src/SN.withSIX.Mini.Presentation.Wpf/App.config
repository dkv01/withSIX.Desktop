  <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <extensions>
    <add assembly="LogentriesNLog" />
  </extensions>
    <targets>
      <target name="logfile" xsi:type="File" fileName="${specialfolder:folder=LocalApplicationData}/SIX Networks/Sync/logs/sync.log" archiveFileName="${specialfolder:folder=LocalApplicationData}/SIX Networks/Sync/logs/sync_{#####}.log" archiveAboveSize="1048576" archiveNumbering="Sequence" maxArchiveFiles="5" concurrentWrites="true" />
      <target name="logentries" type="Logentries" debug="true" httpPut="false" ssl="false" layout="${date:format=ddd MMM dd} ${time:format=HH:mm:ss} ${date:format=zzz yyyy} ${logger} : ${LEVEL}, ${message}" />
    </targets>
    <rules>
      <logger name="SN.withSIX.*" minlevel="Trace" writeTo="logfile" />
      <logger name="SN.withSIX.*" minLevel="Error" appendTo="logentries" final="true" />
    </rules>
  </nlog>
